  "suggest.enableFloat": true,
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "winblend": 15
  },
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "winblend": 15
  },
  // Windows style
  "notification.border": true,
  "notification.winblend": 40,
  //diagnostic
  "diagnostic.errorSign": " ",
